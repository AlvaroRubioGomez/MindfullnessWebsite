<h1>Tus respuestas</h1>
<% for(let i = 0; i < posts.length; i++) { %>
    <% if(posts[i].answerObjArr.length != 0){ %>
    <article>
        <table id="results-table" border="1">
            <thead>
                <tr>
                    <th colspan="3"><%= posts[i].qWording %></th>  
                </tr>                              
            </thead>  
            <tbody>
                <% for(let j = 0; j < posts[i].answerObjArr.length; j++) { %>
                <tr>
                    <td><%= posts[i].answerObjArr[j].usedTime %></td>
                    <td><%= posts[i].answerObjArr[j].ansWording %></td>
                    <td><%= posts[i].answerObjArr[j].ansDate %></td>
                </tr>
                <% } %>
            </tbody>
        </table>                                     
    </article> 
    <% } %>          
<% } %>

